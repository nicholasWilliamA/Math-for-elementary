{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Components/*": [
        "./components/*"
      ],
      "@Constants/*": [
        "./constants/*"
      ],
      "@Functions/*": [
        "./functions/*"
      ],
      "@Interfaces/*": [
        "./interfaces/*"
      ],
      "@Assets/*": [
        "./assets/*"
      ],
      "@Contexts/*": [
        "./contexts/*"
      ],
      "@Enums/*": [
        "./enums/*"
      ],
    },
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
  }
}
